{
  "name": "quo-automation",
  "version": "1.0.0",
  "description": "Automation test cases for the QUO application",
  "main": "./node_modules/.bin/wdio",
  "scripts": {
    "test": "wdio wdio.conf.js --spec login.spec.js",
    "test.sauce": "wdio wdio.saucelabs.conf.js",
    "lint": "eslint .",
    "report": "allure serve report/allure-results",
    "report.ci": "allure generate report/allure-results --clean -o allure-report"
  },
  "keywords": [
    "mocha",
    "uitesting",
    "webdriver",
    "webdriverio",
    "e2e",
    "selenium",
    "test automation",
    "javascript"
  ],
  "author": "Vaibhav Vora <vaibhav@pirimid.com>",
  "license": "MIT",
  "dependencies": {
    "@wdio/cli": "^6.0.13",
    "@wdio/sync": "^6.0.12",
    "chai": "^4.2.0",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/webdriverio": "^5.0.0",
    "@wdio/allure-reporter": "^6.0.12",
    "@wdio/junit-reporter": "^6.0.12",
    "@wdio/local-runner": "^6.0.13",
    "@wdio/mocha-framework": "^6.11.0",
    "@wdio/sauce-service": "^6.0.13",
    "@wdio/selenium-standalone-service": "^6.0.12",
    "@wdio/spec-reporter": "^6.0.12",
    "babel-eslint": "^10.1.0",
    "chromedriver": "^80.0.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-mocha": "^6.3.0",
    "wdio-chromedriver-service": "^5.0.2",
    "wdio-json-reporter": "^2.0.0",
    "wdio-teamcity-reporter": "^2.2.0",
    "wdio-testrail-reporter": "^1.1.5"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
